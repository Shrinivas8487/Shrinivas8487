<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Music Website</title>
    <style>
        body {
            font-family: Arial, sans-serif;
            margin: 0;
            padding: 0;
            background: linear-gradient(to bottom, #1a1a1a, #333);
            color: #fff;
            text-align: center;
        }

        header {
            background: #444;
            padding: 20px;
        }

        header h1 {
            margin: 0;
            font-size: 2.5em;
        }

        main {
            padding: 20px;
        }

        .music-player {
            margin: 20px auto;
            padding: 20px;
            max-width: 400px;
            background: #222;
            border-radius: 10px;
            box-shadow: 0 4px 10px rgba(0, 0, 0, 0.5);
        }

        .music-player button {
            padding: 10px 20px;
            margin: 10px;
            font-size: 1em;
            border: none;
            border-radius: 5px;
            background: #ff6600;
            color: white;
            cursor: pointer;
            transition: background 0.3s;
        }

        .music-player button:hover {
            background: #e65c00;
        }

        footer {
            background: #444;
            padding: 10px;
            position: fixed;
            bottom: 0;
            width: 100%;
        }

        footer p {
            margin: 0;
            font-size: 0.9em;
        }
    </style>
</head>
<body>
    <header>
        <h1>My Music Website</h1>
    </header>
    <main>
        <section class="music-player">
            <h2>Now Playing: Relaxing Tune</h2>
            <audio id="audio" src="https://www.soundhelix.com/examples/mp3/SoundHelix-Song-1.mp3"></audio>
            <button id="playBtn">Play</button>
            <button id="pauseBtn">Pause</button>
        </section>
    </main>
    <footer>
        <p>&copy; 2024 Music Website. All rights reserved.</p>
    </footer>
    <script>
        const audio = document.getElementById('audio');
        const playBtn = document.getElementById('playBtn');
        const pauseBtn = document.getElementById('pauseBtn');

        playBtn.addEventListener('click', () => {
            audio.play();
        });

        pauseBtn.addEventListener('click', () => {
            audio.pause();
        });
    </script>
</body>
</html>
